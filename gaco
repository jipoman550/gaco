#!/usr/bin/env bash
# gaco wrapper script for easy execution

# Get the directory where this script is located
SCRIPT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

# Execute the Python module
# We set PYTHONPATH to include the 'src' directory
PYTHONPATH="$SCRIPT_DIR/src" python3 -m gaco.main "$@"
